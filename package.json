{
  "version": "14.0.3",
  "name": "twemoji-react-assets",
  "description": "A Unicode standard based way to implement emoji across all platforms.",
  "homepage": "https://github.com/twitter/twemoji",
  "main": "./assets/react/index.ts",
  "keywords": [
    "emoji",
    "DOM",
    "parser",
    "images",
    "retina",
    "Twitter",
    "unicode"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "phantomjs-prebuilt": "^2.1.16",
    "uglify-js": "2.6.2"
  },
  "scripts": {
    "build": "./scripts/build.js",
    "deploy": "./scripts/deploy.sh gh-pages",
    "test": "phantomjs ./src/test/testrunner.js",
    "prepublishOnly": "run-s build test deploy",
    "svgr": "svgr --out-dir assets/react ./assets/svg --typescript --index-template index-template.js --filename-case kebab --icon"
  },
  "dependencies": {
    "@svgr/cli": "^8.1.0",
    "fs-extra": "^8.0.1",
    "jsonfile": "^5.0.0",
    "twemoji-parser": "14.0.0",
    "universalify": "^0.1.2"
  },
  "files": [
    "assets/react/*"
  ],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
